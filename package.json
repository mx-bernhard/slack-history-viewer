{
  "name": "slack-history-viewer",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:ssr",
    "dev:ssr": "nodemon --watch server.tsx --watch src/server --ext ts,tsx --exec vite-node server.tsx",
    "build:client": "vite build --outDir dist/client",
    "build:server-render": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:server-logic": "tsc --project tsconfig.node.json",
    "build": "yarn build:client && yarn build:server-render && yarn build:server-logic",
    "start": "NODE_ENV=production node dist/server/server.js",
    "lint": "run-p -c -n \"lint:**\"",
    "lint:prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint:eslint": "eslint",
    "lint:tsc": "tsc -b tsconfig.json",
    "prettify": "prettier --write \"src/**/*.{ts,tsx}\" && eslint . --fix",
    "preview": "vite preview",
    "generate-emoji-map": "node --experimental-strip-types scripts/generate-emoji-map.ts"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@fontsource/roboto": "5.2.6",
    "@mui/icons-material": "7.3.2",
    "@mui/material": "7.3.1",
    "@mui/x-tree-view": "8.10.2",
    "@tanstack/react-query": "^5.71.5",
    "@tanstack/react-router": "1.131.40",
    "@tanstack/react-router-devtools": "1.131.40",
    "@tanstack/zod-adapter": "1.131.40",
    "@types/sqlite3": "^5.1.0",
    "@uidotdev/usehooks": "2.4.1",
    "classnames": "2.5.1",
    "compression": "1.8.1",
    "debounce-collect": "^1.0.2",
    "dotenv": "16.5.0",
    "emoji-datasource-google": "15.1.2",
    "express": "5.1.0",
    "glob": "11.0.3",
    "lodash-es": "4.17.21",
    "micro-memoize": "4.1.3",
    "p-limit": "6.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-virtuoso": "4.14.0",
    "remark-gfm": "^4.0.1",
    "sqlite": "5.1.1",
    "sqlite3": "5.1.7",
    "typed-assert": "1.0.9",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@slack/web-api": "7.9.3",
    "@tanstack/router-plugin": "1.131.40",
    "@types/compression": "1.8.1",
    "@types/express": "^5.0.0",
    "@types/glob": "^8.1.0",
    "@types/lodash-es": "4.17.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "^16.0.0",
    "jiti": "2.5.1",
    "nodemon": "^3.1.9",
    "npm-run-all2": "8.0.4",
    "prettier": "^3.5.3",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0",
    "vite": "7.1.11",
    "vite-node": "3.2.4",
    "vite-plugin-static-copy": "3.1.2"
  },
  "packageManager": "yarn@4.9.2",
  "resolutions": {
    "react-is": "18.3.1",
    "vite": "7.1.11"
  }
}
